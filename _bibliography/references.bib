@article{cm-a07-sms,
	Abstract = {Reduced-order modal models of linear piezoelectric structures are useful in vibration control and health monitoring. We study experimental identification of the fundamental parameters of these modal models. We propose two identification techniques for estimating piezoelectric modal couplings and piezoelectric modal capacitances. Both methods are easily implementable and rely on elementary vibration tests. We show the application of these methods to a sample structure hosting multiple transducers. {\copyright} IOP Publishing Ltd.},
