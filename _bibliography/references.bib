@inproceedings{cm-c09-LazGauPau,
	Abstract = {Formation of basalt columns during cooling of lava may be modeled by the drying of colloidal silica suspension confined in capillary cells (Allain and Limat 1995, Gauthier et al. 2007). During the drying process, particles aggregate at the open edge forming a growing drained gelled porous medium. High negative capillary pressure in the draining fluid (Dufresne et al., 2003) and adhesion to the walls of the cells generates high tensile stresses in the gel leading to crack formation. Depending on the experimental conditions and the shape of the cell (rectangular or circular), several crack morphologies appear. Here the aim is to compare the experimental morphologies with the ones predicted by fracture mechanics. For this purpose, the drained gelled porous medium is modeled by a linear elastic medium subjected to tensile prestresses and the cracks by the variational approach to fracture of Bourdin, Francfort and Marigo (1998, 2000, 2008).},
	Author = {Lazarus, V. and Gauthier, G. and Pauchard, L. and Maurini, C. and Valdivia, C.},
	Booktitle = {12th International Conference on Fracture 2009, ICF-12},
	Date-Added = {2017-08-26 09:45:56 +0000},
	Date-Modified = {2017-08-26 09:45:56 +0000},
	Document_Type = {Conference Paper},
	Keywords = {cm-noarticles, cm-proceedings},
	Pages = {5521-5528},
	Source = {Scopus},
	Title = {Basalt columns and crack formation during directional drying of colloidal suspensions in capillary tubes},
	Url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84869797666&partnerID=40&md5=e5825f640cde019efbda00d5522553c2},
