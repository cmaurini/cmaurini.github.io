@article{cm-a14-jmps,
	Abstract = {Abstract We study fracture and debonding of a thin stiff film bonded to a rigid substrate through a thin compliant layer, introducing a two-dimensional variational fracture model in brittle elasticity. Fractures are naturally distinguished between transverse cracks in the film (curves in 2D) and debonded surfaces (2D planar regions). In order to study the mechanical response of such systems under increasing loads, we formulate a dimension-reduced, rate-independent, irreversible evolution law accounting for both transverse fracture and debonding. We propose a numerical implementation based on a regularized formulation of the fracture problem via a gradient damage functional, and provide an illustration of its capabilities exploring complex crack patterns, showing a qualitative comparison with geometrically involved real life examples. Moreover, we justify the underlying dimension-reduced model in the setting of scalar-valued displacement fields by a rigorous asymptotic analysis using Γ -convergence, starting from the three-dimensional variational fracture (free-discontinuity) problem under precise scaling hypotheses on material and geometric parameters. },
	Author = {Leon Baldelli, A.A. and Babadjian, J.-F. and Bourdin, B. and Henao, D. and Maurini, C.},
	Date-Modified = {2014-09-11 13:19:22 +0000},
	Doi = {10.1016/j.jmps.2014.05.020},
	Issn = {0022-5096},
	Journal = {Journal of the Mechanics and Physics of Solids},
	Keywords = {Dimension reduction, cm-article},
	Note = {\href{http://www.lmm.jussieu.fr/~corrado/articles/cm-a14-LeoBabBou.pdf}{Link to PDF}},
	Number = {0},
	Pages = {320 - 348},
	Title = {A variational model for fracture and debonding of thin films under in-plane loadings},
	Url = {http://www.sciencedirect.com/science/article/pii/S0022509614001112},
