	Journal = {European Journal of Mechanics, A/Solids},
	Keywords = {Bistable structures; Extensible elastica models; Morphing structures; Multiparameter actuation; Postbuckling behavior; Snap-through instability, Bending strength; Buckling; Piezoelectric actuators; Stiffness, Beams and girders, cm-article},
	Note = {\href{http://www.lmm.jussieu.fr/~corrado/articles/cm-a07-MauPouVid.pdf}{Link to PDF}},
	Number = {5},
	Pages = {837-853},
	Source = {Scopus},
	Title = {Distributed piezoelectric actuation of a bistable buckled beam},
	Url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-34447260266&partnerID=40&md5=5bb27cdcaabeb8f89514518a488c0405},
