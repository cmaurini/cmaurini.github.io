	Journal = {Journal of Sound and Vibration},
	Keywords = {Eigenvalues and eigenfunctions; Finite element method; Mathematical models; Numerical methods; Problem solving, Beam theory; Discretization; Infinite dimensional system; Piezoelectric beams, Piezoelectric devices, cm-article},
	Note = {\href{http://www.lmm.jussieu.fr/~corrado/articles/cm-a06-MauPorPou.pdf}{Link to PDF}},
