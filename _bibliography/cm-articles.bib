	Keywords = {cm-article},
	Pages = {1--22},
    pdf={http://www.lmm.jussieu.fr/%7ecorrado/articles/cm-a16-MarMauPha.pdf},
	Title = {An overview of the modelling of fracture by gradient damage models},
